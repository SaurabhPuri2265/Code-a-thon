class Solution {
    public void rotate(int[] a, int k) {
        if(k>a.length)
        	k=k%(a.length);
        int temp,t;
        for(int i=0;i<k;i++)
        {
        	temp=a[a.length-1];
        	
        	for(int j=a.length-2;j>=0;j--)
        	{
        		a[j+1]=a[j];
        		
        	}
        	a[0]=temp;
        }
        
        
    }
}
