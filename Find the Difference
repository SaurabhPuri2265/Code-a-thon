import java.util.*;
class Solution {
    public char findTheDifference(String s, String t) {
        int i=0,j=0;
	String c = null,c1=null;
        char[] d=null;
	
	//Vector<Character> v=new Vector<>();
	
	
	for( i=0;i<t.length();i++)
	{
		c=t.substring(i, i+1);
		j=s.indexOf(c);
		if(j==-1)
		{
            c1=c;
			break;
		}
		
		
	}
	d=c1.toCharArray();
        return d[0];
    }
}
