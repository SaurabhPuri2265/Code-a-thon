class Solution {
    public boolean isAnagram(String s, String s1) {
    if(s.length()!=s1.length())
        return false;    
    char[] s2=s.toCharArray(),s3=s1.toCharArray();
	Arrays.sort(s2);
	Arrays.sort(s3);
    
    
        
	
	for(int i=0;i<s2.length;i++)
	{
		if(s2[i]!=s3[i])
			return false;
	}
	return true;
       
        
    }
}
