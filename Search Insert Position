class Solution {
    public int searchInsert(int[] a, int t) {
        int c=0;
        for(int i=0;i<a.length;i++)
        {
            if(t<a[0])
            {
                return 0;
            }
            if(t>a[a.length-1])
        	{
        		return a.length;
        	}
        	
        	if(a[i]<t && t<a[i+1])
        	{
        		return i+1;
        	}
        	if(a[i]==t)
        	{
        		c=i;
                break;
        	}
        }
        return c;
        
    }
}
