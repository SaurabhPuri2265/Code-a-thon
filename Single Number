class Solution {
    public int singleNumber(int[] a) {
        HashSet<Integer> h=new HashSet<>();
        int c=0,e=0;
        for(int i=0;i<a.length;i++)
        {
        	if(h.contains(a[i]))
        	{
        		h.remove(a[i]);
        	}
        	else
        		h.add(a[i]);
        }
       
        Iterator<Integer> i=h.iterator();
        c=i.next();
        return c;
    }
}
